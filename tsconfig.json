{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "target": "ES2017"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/(with-layout)/layout.tsx",
    "app/layout.tsx",
    "components/functionals/Combobox.jsx",
    "app/(with-layout)/page.tsx",
    "lib/schemas.ts",
    "components/functionals/DownloadButton.jsx",
    "components/functionals/DocPdf.js",
    "components/functionals/EspecialWarningDelete.jsx",
    "app/(with-layout)/products/page.tsx",
    "components/ui/data-table-areas.jsx",
    "components/ui/data-table-salidas.jsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
